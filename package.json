{
  "name": "n8n-dfy-autopilot",
  "version": "1.0.0",
  "description": "Fully automated n8n workflow generation, testing, and delivery system with YouTube content creation",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "db:setup": "node scripts/setup-database.js",
    "db:migrate": "node scripts/migrate.js",
    "migrate:prod": "node scripts/migrate-production.js",
    "build": "npm run build:client && npm run build:server",
    "build:client": "webpack --mode production",
    "build:server": "babel src -d dist",
    "lint": "eslint src/",
    "format": "prettier --write src/"
  },
  "keywords": [
    "n8n",
    "automation",
    "workflow",
    "ai",
    "claude",
    "youtube",
    "video-generation"
  ],
  "author": "Robert Sher",
